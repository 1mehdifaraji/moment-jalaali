{
  "name": "moment-jalaali",
  "version": "0.1.0",
  "description": "A Jalaali (Jalali, Persian, Khorshidi, Shamsi) calendar system plugin for moment.js.",
  "main": "moment-jalaali.js",
  "scripts": {
    "test": "node_modules/.bin/mocha -R spec -u bdd --check-leaks --compilers coffee:coffee-script src/test.coffee",
    "test-watch": "node_modules/.bin/mocha -R spec -u bdd -w --check-leaks --compilers coffee:coffee-script src/test.coffee",
    "prepublish": "node_modules/.bin/coffee -o ./ -c src/moment-jalaali.coffee"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/behrang/moment-jalaali.git"
  },
  "keywords": [
    "jalaali",
    "jalali",
    "persian",
    "khorshidi",
    "shamsi",
    "date",
    "calendar",
    "conversion",
    "convert"
  ],
  "author": "Behrang Noruzi Niya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/behrang/moment-jalaali/issues"
  },
  "devDependencies": {
    "mocha": "~1.12.0",
    "coffee-script": "~1.6.3",
    "chai": "~1.7.2"
  },
  "dependencies": {
    "moment": "~2.1.0"
  }
}
